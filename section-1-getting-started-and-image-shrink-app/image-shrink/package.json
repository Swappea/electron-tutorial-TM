{
  "name": "image-shrink",
  "productName": "ImageShrink",
  "version": "1.0.0",
  "description": "App to optimize images",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=SS --version-string.FileDescription=SS --version-string.ProductName=\"ImageShrink\""
  },
  "author": "Swapnesh Sangle",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.0.2",
    "electron-packager": "^14.2.1",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "electron-log": "^4.2.1",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "slash": "^3.0.0"
  }
}
